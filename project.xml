<project
    name = "jportalsandbox"
    description = "jportal renderer"
    script = "zproject.gsl"
    prefix = "jp"
    header = "jp_head.h"
    license = "MPLv2"
    url = "https://github.com/mrjnumber1/jportal"
    repository = "https://github.com/mrjnumber1/jportal"
    email = "multicoloredwiz+removeme@gmail.com"
    >

    <version major = "0" minor = "0" patch="1"/>
    <include filename = "license.xml" />

    <target name="travis" />
	<target name="mingw32" />
	
	<class name="jp_io_cfgmap" />
	<class name="jp_io_level" />
	<class name="jp_io_chunk" />
	
	<class name="jp_entity_base" />
	<class name="jp_entity_player" parent="entity" />
	<class name="jp_entity_enemy" parent="entity" />
	
    
	
	<class name="jp_math_stopwatch" />
	
	<class name="jp_math_mat" />
	<class name="jp_math_vecxy" />
	<class name="jp_math_vecxyz" />
	<class name="jp_math_plane" />
	<class name="jp_math_quat" />
	<class name="jp_math_view" />
	
    

    <main name="main">j portal sandbox thing!</main>
</project>
